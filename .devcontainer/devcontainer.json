{
	"name": "dev",
	"dockerComposeFile": "../docker-compose.yaml",
	"service": "dev",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"esbenp.prettier-vscode",
				"ms-vscode.vscode-typescript-tsbundle"
			]
		}
	},
	"forwardPorts": [3030],
	"postCreateCommand": "npm install",
	"remoteUser": "node"
}
