{
	"name": "API Development Container",
	"dockerComposeFile": "../docker-compose.yaml",
	"service": "api-dev",
	"workspaceFolder": "/app",
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"esbenp.prettier-vscode",
				"ms-vscode.vscode-typescript-tsbundle"
			]
		}
	},
	"forwardPorts": [3030],
	"postCreateCommand": "npm install"
}
